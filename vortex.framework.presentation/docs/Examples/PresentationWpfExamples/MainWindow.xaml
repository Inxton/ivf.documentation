<Window x:Class="PresentationWpfExamples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mvvm="clr-namespace:PresentationWpfExamples.MVVM"
        xmlns:renderablecontentcontrolexample="clr-namespace:PresentationWpfExamples.CB.RenderableContentControlExample"
        xmlns:fbcounter="clr-namespace:PresentationWpfExamples.CustomizePresentations.fbCounter"
        xmlns:v="http://vortex.mts/xaml"
        xmlns:app="clr-namespace:PresentationWpfExamples"
        xmlns:generic="clr-namespace:PresentationWpfExamples.fbStations.Generic"
        xmlns:menuedcontrol="clr-namespace:PresentationWpfExamples.MenuedControl"
        mc:Ignorable="d"
        DataContext="{x:Static app:App.Plc}"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="AutoGenerated MVVM">
                <mvvm:AutoRenderingView></mvvm:AutoRenderingView>
            </TabItem>
            <TabItem Header="AutoGenerated Code Behind">
                <renderablecontentcontrolexample:AutoRenderingViewCodeBehind/>
            </TabItem>
            <TabItem Header="Renderer">
                <fbcounter:CounterCustomizedView></fbcounter:CounterCustomizedView>
            </TabItem>
            <TabItem Header="Stations">
                <generic:StationsGenerated/>
            </TabItem>
            <TabItem Header="Enumerators">
                <v:RenderableContentControl DataContext="{Binding MAIN._months}" PresentationType="Control"></v:RenderableContentControl>
            </TabItem>
            <TabItem Header="Menu">
                <menuedcontrol:MainMenuView></menuedcontrol:MainMenuView>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
